.data	
	vet1: .word 0,0,0,0,0
	vet2: .word 0,0,0,0,0
	str1: .asciiz "Vetor 1: "
	str2: .asciiz "Vetor 2: "
	quebra: .asciiz "\n"
	espaco: .asciiz " | "
	
.text
        la $a1, vet1		#posiçao inicial do vet1
        la $a2, vet2        	#posiçao inicial do vet2
        
        
        la $s1, vet1  		#inicia s1 com a posição do vetor vet1
        la $s2, vet2		#inicia s2 com a posição do vetor vet2
        la $s4, vet2
	la $s5, vet2
	
        li $t0, 0               #inicia t0 com 0
        li $t1, 0
        li $t3, 0		
        li $t2, 0
        
        
        Preencher1:   
                beq $t0, 5, Ordenar		#ve se o contador é = 5
                addi $t0, $t0, 1 		#incrementa o contador
                
                #leitura da string str1
		la $a0, str1			
		li $v0, 4
		syscall
		
		#leitura de um valor inteiro
		li $v0, 5
		syscall
		
		sw $v0, 0($s1)			#salva v0 na posição s1
		sw $v0, 0($s2)			#salva v0 na posição s2
		addi $s1, $s1, 4		#muda a posição do vetor
		addi $s2, $s2, 4		#muda a posição do vetor
		j Preencher1			#pula para o label Preencher1
		
		
	Ordenar:
		beq $t2, 5, Quebra1		# ve se o contador é = 5
		addi $t2, $t2, 1		#incrementa o contador
		j Sort				#pula para o label Sort
		
	label:
			
		addi $s4, $s4, 4		#muda a posição do vetor
		add $s5, $s4, $zero		# atribui o mesmo valor de s4 a s5
		li $t3, 0			#reseta o valor de t3 para zero
		j Ordenar			#pula para o label Ordenar
		
			
			 
		
	Sort:	
		addi $s5, $s5, 4		# muda a posição do vetor
		beq $t3, 5, label		# ve se o contador é = 5
		addi $t3, $t3, 1		# incrementa o contador
		lw $t4, 0($s4)			#carrega o conteudo contigo no endereço s4 em t4
		lw $t5, 0($s5)			#carrega o conteudo contigo no endereço s5 em t5
		slt $t6, $t4, $t5		#compara se t4 < t5 e armazena o resultado em t6
		beq $t6, 1, Sort		#compara se t6 = 1
		sw $t5, 0($s4)			#Guarda o valor de t5 na posição do vetor s4
		sw $t4, 0($s5)			#Guarda o valor de t4 na posição do vetor s5
		
		j Sort				#Pula para o label Sort
		 	
			
      	Print1:
      		beq $t1, 5, Quebra2		# ve se o contador é = 5
      		addi $t1, $t1, 1		#incrementa o contador
      		
      		#printa a posiçao do vet1
		lw $a0, 0($a1)			
      		li $v0, 1
      		syscall
      		
      		#leitura da string espaco
      		la $a0, espaco
      		li $v0, 4
      		syscall
      		
      		addi $a1, $a1, 4		#muda a posição do vetor
      		j Print1			#pula para o label Print1
      		
      	Print2:
		
		beq $t7, 5, exit
		addi $t7, $t7, 1
		
		#printa a posição do vet2
		lw $a0, 0($a2)	
      		li $v0, 1
      		syscall
      		
      		#leitura da string espaco
      		la $a0, espaco
      		li $v0, 4
      		syscall
      		
      		addi $a2, $a2, 4		#muda a posição do vetor
      		j Print2
      		
      	Quebra1:
      	
      		#leitura da string quebra
      		la $a0, quebra
      		li $v0, 4
      		syscall
      		
      		#leitura da string str1
      		la $a0, str1			
		li $v0, 4
		syscall
		
		#leitura da string quebra
      		la $a0, quebra
      		li $v0, 4
      		syscall
      		
      		j Print1
      		
      	Quebra2:
		la $a0, quebra
      		li $v0, 4
      		syscall      	
      		
      		#leitura da string str2
      		la $a0, str2			
		li $v0, 4
		syscall
		
      		la $a0, quebra
      		li $v0, 4
      		syscall
      		
      		j Print2
      		
      	exit:	
      		#comando para finalizar o programa
      		li $v0, 10
		syscall
